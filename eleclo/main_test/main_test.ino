    if (0 <= i && i < 256) //256番目のピクセルまではフロントの一つ目のパネルまで
    {

      if (((i / 32) % 2) == 0) //((i/32) % 2)==0)
      {
        front_1[i + awase] = CRGB(matrix[i].R, matrix[i].G, matrix[i].B);
        awase -= 2;

      }
      else
      {
        front_1[i] = CRGB(matrix[i].R, matrix[i].G, matrix[i].B);
      }
    }
    else if (256 <= i && i < 512) //二つ目のパネル
