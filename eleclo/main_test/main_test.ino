unsigned int i, locate, odr;

void loop() {
  if ('0' < order && order < '9') {
    led_off();
    locate = 0;
    Serial.println("データ格納します");
    odr = myatoi(order);


    for (i = 0; i < LED_N * 3; i++)
    {
      if (( i % 3 ) == 0)
      {
        matrix[locate].R = RGB_data[odr][i];
      }
      else if ( ( i % 3 ) == 1)
      {
        matrix[locate].G = RGB_data[odr][i];
      }
      else if ( ( i % 3 ) == 2)
      {
        matrix[locate].B = RGB_data[odr][i];
        locate++;
        //        Serial.print(i);
        //        Serial.print("番目 ");
        //        Serial.println("RGBのデータが格納できたので、次の座標の入力に移ります");
      }
    }
    Serial.println("データ格納しました");
    led_change();

    if (order == '4')
    {
      order++;
      delay(1000);
    }
    else if (order == '5')
    {
      order--;
      delay(1000);
    }
    else
    {
      order = 'k';
    }
  }




  else if (order == 'k') {

    //    なにもしない
  } else if (order == 'e') {
    led_off();
    delay(1000);
  }
  else if (order == 't')
  {
    led_test();
  }
  delay(100);
